{
  "id": "ElementsBlock",
  "type": "object",
  "properties": {
    "booleanValue": {
      "type": "boolean",
      "controls": {
        "name": "Boolean",
        "defaultValue": true
      }
    },
    "color": {
      "type": "string",
      "controls": {
        "type": "select",
        "name": "Enum Select",
        "enum": [
          {
            "name": "Red",
            "value": "#ff0000"
          },
          {
            "name": "Green",
            "value": "#00ff00"
          },
          {
            "name": "Blue",
            "value": "#0000ff"
          }
        ],
        "defaultValue": "#0000ff"
      }
    },
    "link": {
      "type": "object",
      "controls": {
        "type": "link",
        "name": "Link",
        "defaultValue": { "name": "", "url": "" }
      },
      "properties": {
        "name": {
          "type": "string",
          "defaultValue": ""
        },
        "url": {
          "type": "string",
          "defaultValue": ""
        }
      }
    }
  }
}
